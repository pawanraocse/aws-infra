# Budget Environment - Example Variables
# Copy this file to terraform.tfvars and customize

# Project
project_name = "saas-factory"
environment  = "budget"
aws_region   = "us-east-1"

# VPC
vpc_cidr           = "10.0.0.0/16"
availability_zones = ["us-east-1a", "us-east-1b"]

# EC2 (Free Tier)
ec2_instance_type       = "t2.micro"
allowed_ssh_cidr_blocks = ["0.0.0.0/0"]  # Allow all IPs as requested
ssh_public_key          = ""

# Amplify (Free Tier)
frontend_repository_url = "https://github.com/pawanraocse/aws-infra"
github_access_token     = ""  # SENSITIVE - use environment variable instead
frontend_branch         = "main"
frontend_app_root       = "frontend"
